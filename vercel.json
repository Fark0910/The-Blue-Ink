{
    "version": 2, 

    "functions": {
        "api//*.{js,ts}": {
          "maxDuration": 60
        }
      },
    

    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
    
  }
  
  